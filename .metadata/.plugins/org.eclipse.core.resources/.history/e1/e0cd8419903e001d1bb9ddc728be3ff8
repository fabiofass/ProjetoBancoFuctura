package BancoFuctura;

public abstract class Conta {
	private String numero_conta;
	private String nome_titular;
	private Double saldo_disponivel = 0.0;
	
	public String getNumero_conta() {
		return numero_conta;
	}
	public void setNumero_conta(String numero_conta) {
		this.numero_conta = numero_conta;
	}
	public String getNome_titular() {
		return nome_titular;
	}
	public void setNome_titular(String nome_titular) {
		this.nome_titular = nome_titular;
	}
	public Double getSaldo_disponivel() {
		return saldo_disponivel;
	}
	public void setSaldo_disponivel(Double saldo_disponivel) {
		this.saldo_disponivel = saldo_disponivel;
	}
	public Conta(String numero_conta, String nome_titular, Double saldo_disponivel) {
		this.numero_conta = numero_conta;
		this.nome_titular = nome_titular;
		this.saldo_disponivel = saldo_disponivel;
	}
	public void exibirContas() {
		
	}
	
		
	
	public void fazerDeposito() {
		
	}
	
	
	public void exibirSaldo() {
		
	}
	@Override
	public String toString() {
		return "Conta [numero_conta=" + numero_conta + ", nome_titular=" + nome_titular + ", saldo_disponivel="
				+ saldo_disponivel + "]";
	}
	

}
